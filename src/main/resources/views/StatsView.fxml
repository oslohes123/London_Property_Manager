<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane style="-fx-background-color: #faedcd;" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.london_property_market.UI.Statistics.StatisticsController">
   <right>
      <Button fx:id="rightStatsButton" mnemonicParsing="false" onMouseClicked="#rightButtonClick" prefHeight="312.0" prefWidth="110.0" style="-fx-border-color: black;" text="&gt;" textAlignment="CENTER" textOverrun="CLIP" BorderPane.alignment="CENTER">
         <font>
            <Font name="Calibri" size="55.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </Button>
   </right>
   <left>
      <Button fx:id="leftStatsButton" mnemonicParsing="false" onMouseClicked="#leftButtonClick" prefHeight="312.0" prefWidth="110.0" style="-fx-border-color: black;" text="&lt;" textAlignment="CENTER" textOverrun="CLIP" BorderPane.alignment="CENTER">
         <font>
            <Font name="Calibri" size="55.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </Button>
   </left>
   <top>
      <Label fx:id="statsTitle" text="StatTitle" BorderPane.alignment="CENTER">
         <font>
            <Font size="32.0" />
         </font>
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </Label>
   </top>
   <center>
      <TableView fx:id="statTable" BorderPane.alignment="CENTER">
        <columns>
        </columns>
         <BorderPane.margin>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </BorderPane.margin>
      </TableView>
   </center>
</BorderPane>
